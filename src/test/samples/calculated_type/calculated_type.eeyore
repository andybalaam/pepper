int config_setting = 0

def type get_number_type( int cfg ):
    if cfg > 0:
        return float
    else:
        return int

# The type of the parameter is calculated by calling a function
def bool gt3( get_number_type(config_setting) x ):
    return x > 3

print( gt3( 4 ) )


