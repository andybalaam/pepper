import sys

def void do_it( int x ):
    print( "int: " + x )

def void do_it( bool x ):
    print( "bool: " + x )

do_it( len( sys.argv ) > 2 )
do_it( len( sys.argv ) )
do_it( len( sys.argv ) )
do_it( len( sys.argv ) > 3 )

